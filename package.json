{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start:prod": "cross-env NODE_ENV=production netlify dev",
    "start": "concurrently \"yarn run start:css\" \"remix dev\"",
    "start:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "build": "yarn run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o src/styles/app.css"
  },
  "dependencies": {
    "@netlify/functions": "^1.3.0",
    "@remix-run/netlify": "^1.7.6",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
